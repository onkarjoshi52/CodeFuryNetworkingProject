<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>ContactAndNetworkingApp</display-name>
  <welcome-file-list>
    <welcome-file>addnewcontact.html</welcome-file>
  </welcome-file-list>
    <servlet>
       <servlet-name>entercontactdata</servlet-name>
       <servlet-class>com.demo.servlet.EnterContactDataServlet</servlet-class>
       <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
       <servlet-name>entercontactdata</servlet-name>
       <url-pattern>/entercontactdata</url-pattern>
  </servlet-mapping>
    <servlet>
       <servlet-name>addcontact</servlet-name>
       <servlet-class>com.demo.servlet.AddContactDetailsServlet</servlet-class>
       <load-on-startup>2</load-on-startup>
  </servlet>
  <servlet-mapping>
       <servlet-name>addcontact</servlet-name>
       <url-pattern>/addcontact</url-pattern>
  </servlet-mapping>
</web-app>